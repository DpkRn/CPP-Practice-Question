{
    "manifest_version": 3,
    "name": "save interview",
    "description": "Runs a script when the user clicks the action toolbar icon.",
    "version": "1.0",
    "icons": { 
      "128": "logo.png"
    },
    "background": {
      "service_worker": "background.js"
    },
    "content_scripts": [
      {
          "matches": ["<all_urls>"],
          "js":["content.js"]
          
      }
  ],
    "action": {
        "default_popup": "popup.html",
      "default_icon": {
        "128": "logo.png"
      }
    },
    "permissions": ["storage","scripting", "activeTab"]
  }